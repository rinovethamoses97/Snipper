{
    "manifest_version":2,
    "name":"Snipper",
    "version":"0.1",
    "description":"",
    "content_scripts":[
        {
            "matches": ["<all_urls>"],
            "js":["p5.js","content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "icons":{
        "16":"icon.png",
        "48":"icon.png",
        "128":"icon.png"
    },
    "browser_action": {
        "default_title": "Snipper",
        "default_popup": "index.html"
    },
    "permissions": [
        "activeTab"
      ]
}